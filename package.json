{
  "name": "@i-xi-dev/ms932-encoder",
  "version": "1.2.6",
  "description": "A JavaScript Windows-31J encoder, implements Shift_JIS encoder defined in WHATWG Encoding Standard.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": "./node/index.mjs",
      "default": "./dist/index.js"
    },
    "./stream": {
      "node": "./node/stream/index.mjs",
      "default": "./dist/stream/index.js"
    }
  },
  "scripts": {
    "build": "tsc --emitDeclarationOnly && webpack",
    "clean": "node -e \"fs.rmSync(`dist`, { recursive:true, force:true })\"",
    "lint": "eslint --ext ts --fix ./src/",
    "test": "mocha test/node/ --recursive --timeout 4000",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-xi-dev/ms932-encoder.es.git"
  },
  "keywords": [
    "ms932",
    "shift_jis",
    "windows-31j",
    "textencoder",
    "textencoderstream",
    "browser",
    "deno",
    "nodejs"
  ],
  "author": "i-xi-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/i-xi-dev/ms932-encoder.es/issues"
  },
  "homepage": "https://github.com/i-xi-dev/ms932-encoder.es#readme",
  "files": [
    "dist",
    "node"
  ],
  "engines": {
    "node": ">=16.5.0"
  },
  "devDependencies": {
    "@i-xi-dev/fundamental": "^0.0.17",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
